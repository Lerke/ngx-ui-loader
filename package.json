{
  "name": "@lerkel/ngx-ui-loader",
  "version": "16.1.2",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{scss,ts,component.html}\"",
    "start": "ng serve",
    "build": "ng build ngx-ui-loader --configuration production && ng build --configuration production",
    "build:lib": "ng build ngx-ui-loader --configuration production && cp README.md CHANGELOG.md ./dist/ngx-ui-loader/",
    "build:schematics": "cd projects/ngx-ui-loader && npm run build && cd ../..",
    "test": "ng test ngx-ui-loader --code-coverage --browsers ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.2",
    "@angular/cdk": "13.3.3",
    "@angular/common": "16.1.2",
    "@angular/compiler": "16.1.2",
    "@angular/core": "16.1.2",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "16.1.2",
    "@angular/material": "13.3.3",
    "@angular/platform-browser": "16.1.2",
    "@angular/platform-browser-dynamic": "16.1.2",
    "@angular/router": "16.1.2",
    "ngx-color-picker": "^14.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "16.1.0",
    "@angular/compiler-cli": "16.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "ng-packagr": "^16.1.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~5.1.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,scss,md}": "prettier --write"
  }
}
